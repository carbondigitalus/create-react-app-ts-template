{
  "name": "semlinks-frontend",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "author": {
    "name": "Carbon Digital",
    "email": "hello@carbondigital.us",
    "url": "https://carbondigital.us/"
  },
  "scripts": {
    "build:dev": "webpack",
    "build:prod": "webpack --config webpack.config.prod.js",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prebuild:clean": "rimraf dist",
    "prebuild:create": "nest build",
    "db:migrate:run": "npm run build && npx typeorm migration:run -d dist/typeorm-cli.config",
    "db:migrate:revert": "npx typeorm migration:revert -d dist/typeorm-cli.config",
    "db:migrate:auto": "npm run build && npx typeorm migration:generate src/migrations/SchemaSync -d dist/typeorm-cli.config",
    "start:dev": "NODE_ENV=development nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "style": "sass public/css/scss/style.scss:public/css/style.css",
    "style:min": "sass public/css/scss/style.scss:public/css/style.css --style=compressed",
    "style:watch": "sass --watch public/css/scss/style.scss:public/css/style.css",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/_setup/jest/jest-e2e.json"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.8.4"
  },
  "type": "module"
}
